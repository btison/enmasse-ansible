{
  "apiVersion": "enmasse.io/v1",
  "kind": "AddressList",
  "items": [
    {
      "metadata": {
        "name": "{{ item.name }}",
        "addressSpace": "{{ address_space_name }}"
      },
      "spec": {
        "address": "{{ item.name }}",
        "type": "{{ item.type }}",
        "plan": "{{ item.plan }}"
      }
    }
  ]
}
